{
    "System": {
        "Config": {
            "Ramdisk": [
                {
                    "#class": "System.Config.Ramdisk",
                    "file": {
                        "extension": {
                            "allow": [
                                "*"
                            ]
                        },
                        "size": {
                            "max": "1M"
                        }
                    },
                    "autoload": {
                        "exclude": {
                            "load": [
                                "Controller\\",
                                "Difference\\Fun\\Module\\Compile"
                            ],
                            "content": [
                                "__DIR__",
                                "__FILE__"
                            ]
                        }
                    },
                    "parse": {
                        "tree": true
                    },
                    "size": "1G"
                }
            ]
        }
    }
}